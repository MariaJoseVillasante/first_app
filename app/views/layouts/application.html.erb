    <%# fin bootstrap %>
    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
  </head>

  <body>
  <nav class="navbar navbar-expand-lg bg-light">
  <div class="container-fluid">
    <a class="navbar-brand" href="#">MJVT</a>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav ms-auto mb-2 mb-lg-0"><%# me-auto cambia por ms-auto %>
        <li class="nav-item">
          <a class="nav-link active" aria-current="page" href="/pages/one">One</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/pages/two">Two</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/pages/three">Three</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/student/index">Student index</a>
        </li>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/pagina/index">Formulario</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/student/edit">Student edit</a>
        </li>
      </ul>
    </div>
  </div>
</nav>
<div class="container">
    <%= yield %>
</div>
    <!-- JavaScript Bundle with Popper desde bootstrap -->
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.5/dist/umd/popper.min.js" integrity="sha384-Xe+8cL9oJa6tN/veChSP7q+mnSPaj5Bcu9mPX5F5xIGE0DVittaqT5lorf0EI7Vk" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.min.js" integrity="sha384-ODmDIVzN+pFdexxHEHFBQH3/9/vQ9uori45z4JjnFsRydbmQbmL5t1tQ0culUzyK" crossorigin="anonymous"></script>
    <%# fin bootstrap %>

  </body>
</html>
